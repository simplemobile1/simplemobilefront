{
	"name": "svelte-commerce",
	"version": "2023.02.01",
	"private": true,
	"scripts": {
		"dev": "vite dev --host --port 3000",
		"build": "vite build",
		"start-node": "node build/index.js",
		"start": "NODE_ENV=production && vite preview --host --port 3000",
		"update": "npm exec -- npx ncu -u -p npm i",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"partytown": "partytown copylib static/~partytown",
		"gen:manifest": "node ./generate-manifest.js",
		"copy": "node devops/copy",
		"liveS": "node devops/deploy",
		"live": "run-s build copy liveS"
	},
	"devDependencies": {
		"@builder.io/partytown": "0.7.5",
		"@sentry/svelte": "7.36.0",
		"@sentry/tracing": "7.36.0",
		"@splidejs/svelte-splide": "0.2.9",
		"@sveltejs/adapter-auto": "2.0.0",
		"@sveltejs/adapter-vercel": "2.0.1",
		"@sveltejs/kit": "1.5.0",
		"@tailwindcss/line-clamp": "^0.4.2",
		"@tailwindcss/typography": "^0.5.9",
		"@types/bcrypt": "^5.0.0",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"@woocommerce/woocommerce-rest-api": "1.0.1",
		"amazon-s3-uri": "^0.1.1",
		"autoprefixer": "^10.4.13",
		"aws-sdk": "^2.1310.0",
		"bcrypt": "^5.1.0",
		"cookie": "^0.5.0",
		"cookie-universal": "^2.2.2",
		"cssnano": "^5.1.14",
		"date-fns": "2.29.3",
		"dayjs": "^1.11.7",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"hash-it": "^6.0.0",
		"ioredis": "5.3.0",
		"minio": "7.0.32",
		"npm-run-all": "4.1.5",
		"postcss": "^8.4.21",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"shelljs": "0.8.5",
		"ssh2shell": "2.0.8",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-confetti": "^1.2.2",
		"svelte-preprocess": "^5.0.1",
		"svelte-share-buttons-component": "1.5.0",
		"svelte-timeago": "0.1.2",
		"svelte-toasts": "^1.1.2",
		"tailwind-scrollbar": "2.1.0",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.5.0",
		"tw-elements": "1.0.0-alpha13",
		"typescript": "~4.9.5",
		"vanilla-lazyload": "^17.8.3",
		"vite": "^4.1.1",
		"zod": "3.20.2",
		"zod-form-data": "1.2.4"
	},
	"type": "module"
}
